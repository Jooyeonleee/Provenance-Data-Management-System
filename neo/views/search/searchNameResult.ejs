            <div class="jumbotron">
                <div class="panel panel-default">
                    <div class="panel-heading" style="font-size:1.4em;">사용자 검색 결과</div>
                    <!-- Table -->


                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>소유자</th>
                                <th>소 속</th>
                                <th>데이터ID</th>
                                <th>가격</th>
                                <th>데이터 종류</th>
                                <th>수집 장치</th>
                                <th>행동</th>
                                <th>날짜</th>
                            </tr>
                        </thead>
                        <tbody>
                        <% for(var i = 0; i < names.length; i++ ) { %>
                            <tr>
                                <th scope="row">
                                    <%= (i + 1) %>
                                </th>
                                <td>
                                    <%= names[i] %>
                                </td>
                                <td align=center>
                                    <%= affiliations[i] %>
                                </td>
                                <td align=center>
                                    <%= dataNames3[i] %>
